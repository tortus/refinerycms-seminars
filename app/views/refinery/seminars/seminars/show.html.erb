<% content_for :body_content_title do %>
  <%= @seminar.name %>
<% end %>

<% content_for :body do %>
  <section class="seminar_content">
    <%=raw @seminar.content %>
  </section>
  <section>
    <h2><%= t('.dates') %></h2>
    <%= render 'dates', :dates => @seminar.dates.active %>
  </section>
<% end %>

<% content_for :side_body do %>
  <%= render 'body_content_right' %>
<% end %>

<%= render '/refinery/content_page' %>
